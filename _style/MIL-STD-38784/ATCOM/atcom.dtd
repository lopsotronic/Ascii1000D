<!-- Removed %verified;, %secur; and revchg %yesorno; '0' attributes for clean up purposes.  KKJ 082905 -->

<!ENTITY % tabatts
"%bodyatt;
tabstyle (oip | rpstl | rpstl7 | partno-list | pm | pmd | pms | toc) #IMPLIED
frame (top | bottom | topbot | all | sides | none ) #IMPLIED -- was 'all' --
colsep %yesorno; #IMPLIED -- was '1' --
rowsep %yesorno; #IMPLIED -- was '0' --
orient (port | land1 | land2) port -- was '#IMPLIED' --
pgwide %yesorno; #IMPLIED -- was '1' --
cellcont (Continued | Cont) 'Continued'
rpstl-indenture ( 0 |1 | 2 | 3 | 4 | 5 | 6 | 7 ) '0'
