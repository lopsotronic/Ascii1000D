%db;
%spacingatt; " >

<!-- Added xid attribute to table entry element list of attributes, Bill Bernickus,  110394 -->
<!-- Added label attribute to table entry element list of attributes, Bill Bernickus,  011895 -->

<!ENTITY % entryatt
"label CDATA #IMPLIED
xid ID #IMPLIED
colname NMTOKEN #IMPLIED   -- was colref --
spanname NMTOKEN #IMPLIED  -- was spanref --
morerows NUMBER '0'
