"chglvl %yesorno; '0'
inschlvl NMTOKEN #IMPLIED
delchlvl NMTOKEN #IMPLIED
revchg %yesorno; '0'">

<!-- Removed "sssn CDATA #IMPLIED inschlvl NMTOKEN #IMPLIED
delchlvl NMTOKEN #IMPLIED" attribute for cleanup purposes.  KKJ 082905 -->
<!ENTITY % bodyatt
"label CDATA #IMPLIED
%db;" >

<!ENTITY % chapatt
"%bodyatt;
xid ID #IMPLIED " >

<!ENTITY % sectatt "%chapatt; " >

<!--  Added spacing attribute to allow for variable prespacing before a para and all the subparas A. Leach 2 Feb 2005 -->

<!-- Removed %verified;, hcp %yesorno; '0', esds %yesorno; '0' and revchg %yesorno; '0' attributes for cleanup purposes. KKJ 082905 -->
<!ENTITY % para0att
"%bodyatt;

emergency (0|1) '0'
