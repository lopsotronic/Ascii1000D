 append the following statement to the beginning of the file:

     <!DOCTYPE doc [ and the associated "]>" to the end of the file. -->

<!-- ENTITY DECLARATIONS -->

<!ENTITY % yesorno "NUMBER" >

<!ENTITY % secur "security (u | c | s) 'u'" >

<!-- Removed verified entity for clean up purposes KKJ 082905 
<!ENTITY % verified
"verdate CDATA #IMPLIED
verrem CDATA #IMPLIED" > -->

<!-- Removed itemid entity for cleanup purposes.  KKJ 082905
<!ENTITY % itemid
"unit CDATA #IMPLIED
module CDATA #IMPLIED
lru CDATA #IMPLIED
compon CDATA #IMPLIED
partno CDATA #IMPLIED
refdes CDATA #IMPLIED" > -->

<!-- Removed "skilltrk NMTOKENS #IMPLIED, assocfig IDREFS #IMPLIED and assoctab IDREFS #IMPLIED" for cleanup purposes.  KKJ 082905 -->
<!ENTITY % content
"contype (desc | proc | part) #IMPLIED" >

<!-- Removed reference to itemid for cleanup purposes.  KKJ 082905 -->
<!ENTITY % db "%content;" >

<!ENTITY % docatt
"docid CDATA #IMPLIED
docstat (revision | change | prelim | draft | formal | routine | urgent | emergency) 'prelim'
verstatpg (all | unverified) 'all'
columns (one|two) 'two'
cover %yesorno; '0'
%secur; " >

<!-- Removed %change entity for cleanup purposes.  KKJ 082905
<!ENTITY % change
"inschlvl NMTOKEN #IMPLIED
delchlvl NMTOKEN #IMPLIED
revchg %yesorno; '0'" > -->

