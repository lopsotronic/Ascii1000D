<!ELEMENT title     - -  (%text;)   -(%exclus;) >
<!ATTLIST title wcnid ID #IMPLIED 
								wcnref IDREFS #IMPLIED
								%spacingatt;
								%change; >

<!-- Added tmno for use in chginssht so ever-changing tm numbers may be displayed to match legacy data. KKJ 032310 -->
<!ELEMENT tmno     - -  (%text;)   -(%exclus;) >

<!-- Removed %change; and %secur; attributes for cleanup purposes. KKJ 082905 -->
<!ELEMENT tmidno     - -  (%text;)   -(%exclus;) >
<!ATTLIST tmidno     service %service; #IMPLIED >


<!ELEMENT tmimprep     - -  (%parazero;, subpara1*) >
<!ATTLIST tmimprep     %para0att; >

<!-- Added topic as part of the index Allissa Leach 11 July 2008--> 
<!-- Changed #PCDATA to %text so rev-video, change, emphasis, etc. are available within it.  Karen Junkin 20 Oct 2008 -->

<!ELEMENT topic - o (%text) >

<!ELEMENT totalpgs	- - (#PCDATA) >

<!ELEMENT tpdr     - -  (activity, tpdrno, location)+ >

<!-- Removed  and %secur for attribute on tpdrno, typedes, typeno and verbatim for cleanup process. KKJ 093005 -->
<!ELEMENT tpdrno     - o  (#PCDATA) >

<!-- Removed  and %secur for attribute cleanup process. KKJ 093005 -->
<!ELEMENT typedes     - o  (%text;)   -(%exclus;) >

<!ELEMENT typeno     - o  (%text;)   -(%exclus;) >

<!ELEMENT verbatim     - -  CDATA >
<!ATTLIST verbatim     allowbrk %yesorno; "1" >


<!ELEMENT verstat     - o  EMPTY >

<!ELEMENT volnot     - o  (#PCDATA) >

<!ELEMENT volnum     - o  (#PCDATA) >

<!ELEMENT volume     - -  ((docpart, docpart+) | ((front |
                            auxfront), body, rear?)) +(pgbrk | brk | subjinfo) >
<!ATTLIST volume     %bodyatt; >


<!-- Added attribute to turn off warning header for legacy purposes  KKJ 041405 -->
<!-- Removed %change; attribute for cleanup purposes. KKJ 082905 -->
<!ELEMENT warning      - -  (icon?, title?, para*)   -( %exclus; |
                             warning | caution | precaut) >
<!ATTLIST warning     wcnref IDREFS #IMPLIED
                      show-header (0 | 1) "1"
                      centered-para %yesorno; #IMPLIED
														%spacingatt; >

<!-- Optional warning, caution, note added to definition of warnpage to allow for 
DMWRs which have boilerplate definitions of each of these icons on the warning page.  
This is being handled as a declared public entity. -->
<!-- Added wpgcaution to allow older books to list caution notices following the
wpgentry listing.  KKJ 06-07-04 -->

<!-- Removed %verified;, inschlvl NMTOKEN #IMPLIED and delchlvl NMTOKEN #IMPLIED attributes for cleanup purposes. KKJ 082905 
Added title and para for intro information. KKJ 052206 -->
<!-- Changed para? to be para* to match legacy data during conversions. 08 feb 2008 KKJ -->
<!ELEMENT warnpage     - o  (title?, (table | para)*, warning?, caution?, note?, wpgentry+, wpgcaution*) >

<!-- Removed %change; attribute for cleanup purposes. KKJ 082905 -->
<!ELEMENT wpgcaution	 - o (title?, para*)   -(%list; | %exclus; | warning | caution | precaut) >
<!ATTLIST wpgcaution   wcnref IDREFS #IMPLIED 
												 centered-para %yesorno; #IMPLIED>

<!-- Added warning exclusions for wpgentry element, Bill Bernickus, 020195 --> 

<!ELEMENT wpgentry     - o  (icon?, title?, (para | randlist | table)+)  -(seqlist | figure |
                                              foldout | precaut | warning | caution )>
<!ATTLIST wpgentry     warnbox %yesorno; "1"
                       centered-para %yesorno; #IMPLIED >


<!ELEMENT xref     - o  EMPTY >
<!ATTLIST xref     xref IDREF #REQUIRED >
